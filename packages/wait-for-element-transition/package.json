{
  "name": "wait-for-element-transition",
  "version": "1.0.0",
  "description": "Easily wait for an element to complete it's transition using native vanilla JavaScript",
  "main": "index.js",
  "scripts": {
    "test": "bt test",
    "start": "bt server",
    "preversion": "npm test",
    "minify": "uglifyjs dist/wait-for-element-transition.js -o dist/wait-for-element-transition-min.js",
    "build-dist": "rm -rf dist/* && browserify wait-for-element-transition.js -o dist/wait-for-element-transition.js -t [ babelify ] --standalone waitForElementTransition && npm run minify",
    "version": "npm run build-dist && git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkay581/wait-for-element-transition.git"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "author": "Mark Kennedy <mark@heyimmark.com> (heyimmark.com)",
  "keywords": [
    "css transition",
    "neon animation",
    "css animation",
    "polymer transition",
    "element transition",
    "js transition",
    "javascript",
    "wait for transition",
    "wait transition",
    "wait",
    "polymer element",
    "ember transition",
    "transitions",
    "react transition"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mkay581/wait-for-element-transition/issues"
  },
  "homepage": "https://github.com/mkay581/wait-for-element-transition#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-es6-promise": "^1.1.1",
    "babelify": "^9.0.0",
    "browserify": "^16.2.2",
    "build-tools": "^7.2.4",
    "sinon": "^5.1.1",
    "uglify-js": "^3.1.10"
  }
}
